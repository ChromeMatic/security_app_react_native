{
  "expo": {
    "name": "security_mobile_app",
    "slug": "securityapp",
    "scheme": "security_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Security Mobile App to use your location."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/KodeMono-Bold.ttf",
            "./assets/fonts/KodeMono-Medium.ttf",
            "./assets/fonts/KodeMono-Regular.ttf",
            "./assets/fonts/KodeMono-SemiBold.ttf",
            "./assets/fonts/KodeMono-VariableFont_wght.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "83473cbb-25b8-4e83-94fc-0f9bf277640f"
      }
    },
    "owner": "codematic"
  }
}
